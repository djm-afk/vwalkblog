        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.vwalkblog.mapper.BlogCategoryMapper">

    <resultMap id="BaseResultMap" type="com.example.vwalkblog.pojo.BlogCategory">
